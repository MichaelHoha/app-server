// models/codeBlock.js
const { DataTypes } = require('sequelize');
const sequelize = require('../config/database');

const CodeBlock = sequelize.define('CodeBlock', {
  title: {
    type: DataTypes.STRING,
    allowNull: false
  },
  content: {
    type: DataTypes.TEXT,
    allowNull: false
  }
});

module.exports = CodeBlock;
